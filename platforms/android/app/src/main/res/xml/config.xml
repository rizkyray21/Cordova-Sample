<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moengage.cordova.sample" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>moengage-demo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <resource-file src="google-services.json" target="app/google-services.json" />
    <framework src="com.google.firebase:firebase-messaging:17.0.0" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="MoEngage">
        <param name="android-package" value="com.moengage.cordova.MoECordova" />
    </feature>
    <preference name="android-minSdkVersion" value="19"/>
</widget>
